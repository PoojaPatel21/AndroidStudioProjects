<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Preview3">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

</LinearLayout>
    <TextView
        android:id="@+id/name3"
        android:layout_width="match_parent"

        android:layout_height="53dp"
        tools:ignore="InvalidId"></TextView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/addr3"
            android:layout_width="200dp"
            android:layout_height="60dp">

        </TextView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/phone3"
                android:layout_width="match_parent"
                android:layout_height="30dp"

                >

            </TextView>

            <TextView
                android:id="@+id/email3"
                android:layout_width="match_parent"
                android:layout_height="30dp"

                >

            </TextView>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/Educational3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="College"
            android:textSize="24sp">


        </TextView>

        <LinearLayout
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/clgnm3"
                android:layout_width="180dp"
                android:layout_height="40dp"
                android:textAlignment="textStart">

            </TextView>

            <TextView
                android:id="@+id/clggrade3"
                android:layout_width="231dp"
                android:layout_height="40dp"
                android:textAlignment="textStart">

            </TextView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            </LinearLayout>


        </LinearLayout>

        <TextView
            android:id="@+id/clgyop3"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAlignment="textStart">

        </TextView>


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/Jrcollege3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Secondary Education"
            android:textSize="24sp">


        </TextView>

        <LinearLayout
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/jrclgnm3"
                android:layout_width="180dp"
                android:layout_height="40dp"
                android:textAlignment="textStart">

            </TextView>

            <TextView
                android:id="@+id/jrclggrade3"
                android:layout_width="231dp"
                android:layout_height="40dp"
                android:textAlignment="textStart">

            </TextView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            </LinearLayout>


        </LinearLayout>

        <TextView
            android:id="@+id/jrclgyop3"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAlignment="textStart">

        </TextView>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/School3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Primary Education"
            android:textSize="24sp">


        </TextView>

        <LinearLayout
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/schoolnm3"
                android:layout_width="180dp"
                android:layout_height="40dp"
                android:textAlignment="textStart">

            </TextView>

            <TextView
                android:id="@+id/schoolgrade3"
                android:layout_width="231dp"
                android:layout_height="40dp"
                android:textAlignment="textStart">

            </TextView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            </LinearLayout>


        </LinearLayout>

        <TextView
            android:id="@+id/schoolyop3"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:textAlignment="textStart">

        </TextView>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/WorkExp3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Work Experience"
            android:textSize="24sp">


        </TextView>

        <LinearLayout
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/companyname3"
                android:layout_width="180dp"
                android:layout_height="40dp">

            </TextView>

            <TextView
                android:id="@+id/internshiprole3"
                android:layout_width="231dp"
                android:layout_height="40dp">

            </TextView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            </LinearLayout>


        </LinearLayout>

        <TextView
            android:id="@+id/internshipdur3"
            android:layout_width="match_parent"
            android:layout_height="40dp">

        </TextView>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="92dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/Extra3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Extra Curricular Activity"
            android:textSize="24sp">


        </TextView>

        <TextView
            android:id="@+id/Details3"
            android:layout_width="match_parent"
            android:layout_height="57dp">

        </TextView>

    </LinearLayout>


</LinearLayout>
